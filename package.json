{
  "name": "iterare",
  "version": "1.0.0",
  "description": "Array methods for ES6 Iterators",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "lint": "npm run prettier && npm run tslint",
    "tslint": "tslint -c tslint.json -p tsconfig.json 'src/**/*.ts'",
    "prettier": "prettier --write --list-different '**/*.{ts,json,js,md}'",
    "test": "mocha -r source-map-support/register lib/**/*.test.js",
    "cover": "nyc mocha -r source-map-support/register lib/**/*.test.js",
    "build": "tsc",
    "watch": "tsc -w",
    "typedoc": "typedoc --out typedoc --tsconfig tsconfig.json --ignoreCompilerErrors --mode file --excludeExternals src",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "iterator",
    "iteration",
    "functional",
    "es6",
    "collection",
    "array",
    "map",
    "set",
    "filter",
    "reduce",
    "flatten",
    "concat",
    "every",
    "some"
  ],
  "author": "Felix Becker <felix.b@outlook.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/felixfbecker/iterare"
  },
  "bugs": {
    "url": "https://github.com/felixfbecker/iterare/issues"
  },
  "license": "ISC",
  "devDependencies": {
    "@reactivex/ix-es2015-cjs": "^2.2.0",
    "@types/benchmark": "^1.0.30",
    "@types/lodash": "^4.14.55",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.8",
    "benchmark": "^2.1.3",
    "commitlint": "^5.2.5",
    "husky": "^0.14.3",
    "ix": "^2.2.0",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "prettier": "^1.9.2",
    "rxjs": "^5.2.0",
    "semantic-release": "^11.0.2",
    "source-map-support": "^0.4.14",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "typedoc": "^0.9.0",
    "typescript": "~2.6.2"
  },
  "nyc": {
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "lib/**/*.test.js"
    ]
  },
  "release": {
    "verify-conditions": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
